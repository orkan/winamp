{
	"name": "orkan/winamp",
	"description": "Winamp Media Library CLI tools",
	"keywords": [
		"batch",
		"cli",
		"console",
		"m3u",
		"m3u8",
		"php",
		"playlist",
		"winamp"
	],
	"homepage": "https://github.com/orkan/winamp",
	"license": "MIT",
	"authors": [
		{
			"name": "Orkan",
			"role": "Lead",
			"email": "orkans+winamp@gmail.com"
		}
	],
	"require": {
		"symfony/console": "^5.2",
		"symfony/monolog-bridge": "^5.2",
		"james-heinrich/getid3": "^1.9",
		"symfony/finder": "^5.2",
		"orkan/utils": "^7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"symfony/filesystem": "^5.2"
	},
	"autoload": {
		"psr-4": {
			"Orkan\\Winamp\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Orkan\\Winamp\\Tests\\": "tests"
		}
	},
	"bin": [
		"winamp"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "5.4.x-dev",
			"dev-develop": "5.x-dev"
		}
	}
}
